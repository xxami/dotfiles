! green
*.color2:       #a3015c
*.color10:      #b20379

! yellow
*.color3:       #d4217f
*.color11:      #e22466

! blue
*.color4:       #e83bd2
*.color12:      #dd48c2

! magenta
*.color5:       #e54391
*.color13:      #e44879

! cyan
*.color6:       #f879de
*.color14:      #ff71f6

! white
*.color7:       #fb8cdb
*.color15:      #fd8fe8

