
URxvt*.transparent: true
URxvt*.shading: 6
URxvt.scrollBar: false
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/chromium-browser
URxvt.matcher.button: 1
URxvt.font: xft:Terminus-9
URxvt.internalBorder: 8

